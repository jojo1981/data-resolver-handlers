{
  "name": "jojo1981/data-resolver-handlers",
  "description": "Handlers for the data resolver",
  "keywords": [
    "data",
    "resolver",
    "handler",
    "handlers",
    "doctrine"
  ],
  "type": "library",
  "license": "MIT",
  "readme": "README.md",
  "authors": [
    {
      "name": "Joost Nijhuis",
      "email": "jnijhuis81@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": "^8.0",
    "dg/bypass-finals": "^1.3",
    "doctrine/collections": "^1.6",
    "jojo1981/data-resolver": "^2.0",
    "jojo1981/typed-collection": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Jojo1981\\DataResolverHandlers\\": "src/"
    }
  },
  "require-dev": {
    "phpspec/prophecy-phpunit": "^2.0",
    "phpunit/phpunit": "^9.5",
    "roave/security-advisories": "dev-latest"
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\Jojo1981\\DataResolverHandlers\\": "tests/"
    }
  },
  "config": {
    "bin-dir": "bin",
    "sort-packages": true
  },
  "scripts": {
    "test": "bin/phpunit"
  }
}
